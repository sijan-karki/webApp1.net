@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelper;
@model WebApp1BySijan.Models.Student;

<h1>Student Detail</h1>

<table class="table">
    <tr>
        <th>Student ID</th>
        <td>@Model.StdID</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Faculty</th>
        <td>@Model.Faculty</td>
    </tr>
</table>
